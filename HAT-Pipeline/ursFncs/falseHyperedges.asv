function [F] = falseHyperedges(HGtrue, U)
%FALSEHYPEREDGES Generate false hyperedges for hyperedge prediction
%
% Auth: Joshua Pickard
%       jpic@umich.edu
% Date: January 29, 2023

alpha = 0.5;

F = cell(length(U), 1);
for i=1:length(U)
    e = HGtrue.IM(:, U(i));
    n = round(alpha * length(e))
    randsample(1:length(e), n)
    length(e)
    F{i} = e;
end
end

